<?xml version='1.0' encoding='utf-8'?>
<widget id="com.geneoitc.webView" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Push notifications and Web View</name>
    <description>
        Push notifications with firebase and Web View
    </description>
    <author email="j.barcenalumbreras@gmail.com" href="http://www.jorgebarcenadev.com">
        Jorge Barcena Lumbreras
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
    <preference name="android-minSdkVersion" value="26" />
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
